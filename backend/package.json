    "express-validator": "^7.3.1",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "mysql2": "^3.6.5",
